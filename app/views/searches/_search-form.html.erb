<% if user_signed_in? %>
<div class= "form-group">
  <%= form_with url: search_path, method: :get, local:true do |f| %>
    <%= f.text_field :contact %>
    <%= select_tag 'model', options_for_select([['User', 'user'], ['Book', 'book']]) %>
    <%= select_tag 'method', options_for_select([["完全一致","perfect_match"], ["前方一致", "foward_match"], ["後方一致", "backward_match"], ["部分一致", "partial_match"]]) %>
    <%= f.submit '検索' %>
  <% end %>
</div>
<% end %>